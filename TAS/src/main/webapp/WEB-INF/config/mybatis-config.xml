<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
	<!-- fundamental -->
		<typeAlias type="fundamental.dto.BpsDTO" alias="BpsDTO"/>
		<typeAlias type="fundamental.dto.EpsDTO" alias="EpsDTO"/>
		<typeAlias type="fundamental.dto.EnterpriseFinanceDTO" alias="EnterpriseFinanceDTO"/>
