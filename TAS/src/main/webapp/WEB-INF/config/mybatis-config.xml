	<!-- board -->	
		<typeAlias type="board.dto.BoardDTO" alias="brd"/>
	<!-- member -->
		<typeAlias type="member.dto.MemberDTO" alias="member"/>
	<!-- technical -->
		<typeAlias type="technical.dto.StockDInfoDTO" alias="stockdinfo"/>
	<!-- bigdata -->
		<typeAlias type="bigdata.dto.BigdataDTO" alias="bigdata"/>
	
	</typeAliases>
	<mappers>
	<!-- fundamental -->
		<mapper resource="mapper/FundamentalMapper.xml"/>
	<!-- board -->
		<mapper resource="mapper/BoardMapper.xml"/>
	<!-- board2 -->
		<mapper resource="mapper/Board2Mapper.xml"/>
	<!-- member -->	
		<mapper resource="mapper/MemberMapper.xml"/>
